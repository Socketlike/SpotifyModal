{
  "name": "SpotifyModal",
  "version": "1.1.5",
  "description": "A Post-SWC Replugged plugin that shows a little modal on your user dock that lets you see & control what you're playing on Spotify.",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "build:dev": "replugged build plugin",
    "build:dev:bundle": "replugged bundle plugin",
    "build:prod": "replugged build plugin --production",
    "build:prod:bundle": "replugged bundle plugin --production",
    "watch": "replugged build plugin --watch",
    "check": "tsc --noEmit",
    "prettier:check": "prettier ./src --check",
    "eslint:check": "eslint ./src",
    "prettier:fix": "prettier ./src --write",
    "eslint:fix": "eslint ./src --fix",
    "lint": "pnpm run prettier:check && pnpm run eslint:check && pnpm run check",
    "lint:fix": "pnpm run prettier:fix && pnpm run eslint:fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "eslint-config-dmitmel": "github:dmitmel/eslint-config-dmitmel",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.32.1",
    "prettier": "^2.8.3",
    "replugged": "4.0.0-beta0.25",
    "typescript": "^4.9.4",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10"
  }
}
